        "drupal/field_formatter_class": "^1.5",
        "drupal/field_group": "^3.1",
        "drupal/flood_control": "^2.2",
        "drupal/focal_point": "^1.5",
        "drupal/formtips": "^1.5",
        "drupal/gin": "^3.0-alpha36",
        "drupal/gin_lb": "^1.0.0-beta11",
        "drupal/gin_login": "^1.0-rc8",
        "drupal/gin_toolbar": "^1.0-beta19",
