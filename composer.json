{
    "name": "acquia/drupal-minimal-project",
    "description": "Minimal Acquia-compatible Drupal application based on the Drupal Recommended Project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "blazy": {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/refs/tags/1.8.2.zip",
                    "type": "zip"
                }
            }
        },
        "slick": {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/refs/tags/v1.8.1.zip",
                    "type": "zip"
                }
            }
        },
        "chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        }
    },
    "require": {
        "acquia/blt": "^12",
        "acquia/blt-phpcs": "^1.0",
        "acquia/drupal-spec-tool": "*",
        "acquia/memcache-settings": "*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "d8-contrib-modules/cloudflarephpsdk": "1.0.0-alpha5",
        "dinbror/blazy": "^1.8",
        "drupal/access_unpublished": "^1.3",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_lift": "~4.0",
        "drupal/acquia_purge": "^1.1",
        "drupal/acquia_search": "^3.0.0",
        "drupal/address": "^1.9",
        "drupal/addtoany": "^1.15",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/allowed_formats": "^1.3",
        "drupal/autocomplete_deluxe": "^2.0",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/blazy": "^2.4",
        "drupal/block_blacklist": "^1.1",
        "drupal/block_content_permissions": "^1.10",
        "drupal/block_field": "^1.0-rc2",
        "drupal/block_list_override": "^1.0.1",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_accordion": "^1.3",
        "drupal/ckeditor_blockquote_attribution": "^1.0",
        "drupal/cloudflare": "^1.0@beta",
        "drupal/codemirror_editor": "^1.7",
        "drupal/components": "^2.4",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.7",
        "drupal/contact_block": "^1.5",
        "drupal/content_translation_redirect": "^1.3",
        "drupal/cookieconsent": "^1.6",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/default_content": "^2.0.x-dev",
        "drupal/devel": "^4.1",
        "drupal/disable_language": "^1.0@beta",
        "drupal/dropdown_language": "^3.0.0",
