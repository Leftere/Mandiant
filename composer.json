        "drupal/helper": "^1.7",
        "drupal/honeypot": "^2.0",
        "drupal/iframe": "^2.11",
        "drupal/imagemagick": "^3.2",
        "drupal/inline_block_title_automatic": "^1.0",
        "drupal/inline_entity_form": "^1.0-rc9",
        "drupal/key": "^1.14",
        "drupal/layout_builder_additions": "^1.0.0",
        "drupal/layout_builder_at": "^2.12",
